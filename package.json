{
  "name": "vue-command",
  "version": "6.2.1",
  "description": "A fully working Vue.js terminal emulator.",
  "private": false,
  "main": "./dist/vue-command.common.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndabAP/vue-command"
  },
  "keywords": [
    "vue",
    "terminal",
    "emulator"
  ],
  "files": [
    "dist/*",
    "*.js",
    "*.css"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:docs",
    "build:lib": "vue-cli-service build --target lib --name vue-command ./src/index.js",
    "build:docs": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint:fix",
    "test": "vue-cli-service test:unit",
    "serve": "vue-cli-service serve"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "tar": ">=4.4.8",
    "vue": "^2.5.21",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "flush-promises": "^1.0.2",
    "js-yaml": "^3.13.1",
    "querystringify": "^2.1.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.35.2"
  }
}
