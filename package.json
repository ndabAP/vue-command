{
  "name": "vue-command",
  "version": "4.1.0",
  "description": "A fully working Vue.js terminal emulator.",
  "private": false,
  "main": "./dist/vue-command.common.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndabAP/vue-command"
  },
  "files": [
    "dist/*",
    "*.js",
    "*.css"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:docs",
    "build:lib": "vue-cli-service build --target lib --name vue-command ./src/index.js",
    "build:docs": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint:fix"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.21",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.29.0"
  }
}
