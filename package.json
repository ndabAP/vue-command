{
  "name": "vue-command",
  "version": "12.0.3",
  "description": "A fully working, most feature-rich Vue.js terminal emulator.",
  "private": false,
  "main": "./dist/vue-command.common.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndabAP/vue-command"
  },
  "keywords": [
    "bash",
    "emulator",
    "sh",
    "terminal",
    "vue",
    "vuejs"
  ],
  "files": [
    "dist/*.css",
    "dist/*.css.map",
    "dist/*.js",
    "dist/*.js.map"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:docs",
    "build:lib": "vue-cli-service build --target lib --name vue-command ./src/library.js",
    "build:docs": "vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint:fix",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "yargs-parser": "^13.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.32",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.3.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.2.3",
    "js-yaml": "^3.13.1",
    "node-sass": "^4.12.0",
    "querystringify": "^2.1.1",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "~2.1.1",
    "tar": ">=4.4.10",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
